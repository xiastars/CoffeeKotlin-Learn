<?xml version="1.0" encoding="utf-8"?><!--- 有关于TextView的样式 -->
<resources>
    <!-- 垂直的一条线 -->
    <style name="line_vertical">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">1dp</item>
        <item name="android:background">@color/grey_f3</item>
    </style>
    <!-- 平行的一条线 -->
    <style name="line_horizontal">
        <item name="android:layout_height">0.5dip</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/grey_f3</item>
    </style>

    <style name="grey4a_16">
        <item name="android:textColor">@color/grey_4a</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


    <style name="grey99_12">
        <item name="android:textColor">@color/grey_99</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


    <style name="white_12">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


    <style name="white_14">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="white_16">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="grey93_12">
        <item name="android:textColor">@color/grey_93</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="grey93_14">
        <item name="android:textColor">@color/grey_93</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="common_title_yellowff">
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/yellow_ff9</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="line_horizontal_f1">
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/grey_f3</item>
    </style>

</resources>